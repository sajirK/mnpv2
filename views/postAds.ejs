                    <!-- Header Formulaire -->
                    <section class="portfolio" id="portfolio">
                      <div class="container">
                        <div class="row">
                          <div class="card card-register mx-auto mt-5">
                            <div class="card-header">Post your ad</div>
                            <div class="card-body">
                              <!-- formulair -->
                              <form action="/ad" method="post">
                                <div class="form-group">
                                  <div class="form-row">



                                      <div class="btn-group">
                                        <label>Choose a Crypto from this list:
                                      <input  list="Crypto" name="crypto" /></label>
                                      <datalist id="Crypto">
                                        <%
                                        for (var i=1 ; i<scrap.length ; i++) {
                                        %>
                                        <option value="<%=scrap[i].name%>">
                                        <%}%>
                                      </datalist>

                                            </div>
                                          <div class="col-md-6">
                                            <label>Number of seat:</label>
                                            <select name="NbSeat">
                                              <option value="1">1</option>
                                              <option value="2">2</option>
                                              <option value="3">3</option>
                                              <option value="4">4</option>
                                              <option value="5">5</option>
                                              <option value="6">6</option>
                                              <option value="7">7</option>
                                              <option value="8">8</option>
                                              <option value="9">9</option>
                                              <option value="10">10</option>
                                              </select>
                                              </div>


                                  </div>
                                </div>
                                <div class="form-group">
                                  <div class="form-row">
                                    <div class="col-md-6">
                                      <label for="exampleInputPassword1">Title</label>
                                      <input name="title" class="form-control" id="exampleInputPassword1" type="text" placeholder="ads title">
                                    </div>
                                  </div>
                                  </div>
                                <div class="form-group">
                                  <label for="exampleInputEmail1">Message</label>
                                   <textarea name="message" class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="your message"></textarea>

                                </div>

                                <button type="submit" class="btn btn-primary btn-block" >Post</button>
                              </form>
                            </div>
                          </div>

                        </div>
                      </div>
                      </section>

